[
  {
    "request": {
      "url": "^/api/items$",
      "method": "GET",
      "query": {
        "q": ".*" 
      }
        
    },
    "response": {
      "status": 200,
      "headers": {
        "Content-Type": "application/json"
      },
      "file": "./products_search.json"
    }
  },
  {
    "request": {
      "url": "^/api/items/.*",
      "method": "GET"
    },
    "response": {
      "status": 200,
      "headers": {
        "Content-Type": "application/json"
      },
      "file": "./product_id.json"
    }
  }
]
